#Ch 1 - W3-Schools Review - Variables & Expressions
def main():
  x = 5
  y = "John"
  print(x)
  print(y)
  
#type can change
  x = 4       # x is of type int
  x = "Sally" # x is now of type str
  print(x)

#typecasâ€ ing
  x = str(3)    # x will be '3'
  y = int(3)    # y will be 3
  z = float(3)  # z will be 3.0

#type function
  print(type(x))
  print(type(y))

#single/double quotes work the same, just match them
  x = "John"
  # is the same as
  x = 'John'

#Names are case-sensitive
  a = 4
  A = "Sally"
  #A will not overwrite a

main()
